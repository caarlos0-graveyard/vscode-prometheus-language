{
    "name": "language-prometheus",
    "displayName": "language-prometheus",
    "description": "Prometheus .rules support for vscode",
    "version": "0.0.4",
    "publisher": "caarlos0",
    "repository": {
        "type": "git",
        "url": "https://github.com/caarlos0/vscode-prometheus-language.git"
    },
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "prometheus",
                "aliases": [
                    "prometheus"
                ],
                "extensions": [
                    ".rules"
                ]
            }
        ],
        "grammars": [
            {
                "language": "prometheus",
                "scopeName": "source.rules",
                "path": "./syntaxes/prometheus.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "prometheus",
                "path": "./snippets/prometheus.json"
            }
        ]
    }
}